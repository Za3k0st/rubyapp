!!!
%html
  %head
    %title
      = full_title(yield(:title))
    %script(src="https://www.gstatic.com/charts/loader.js")
    %script(src="https://www.google.com/jsapi")
    = stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true
    = javascript_include_tag 'application', 'data-turbolinks-track' => true
    = favicon_link_tag 'favicon.png'
    = csrf_meta_tags
    = csrf_meta_tags
    = stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload'

    = javascript_include_tag 'application', 'data-turbolinks-track': 'reload'
  %body
    %header.navbar.navbar-top.navbar-inverse{'style' => "border-radius:0px; background-color: #7dbedd; border-color: #7dbedd;"}
      .nav{'style' => "padding:10px;"}
        .ul.nav.navbar-nav.navbar-left
          =image_tag("shared/logo.png", class:"img-responsive", style:"width: 400px; ")
        .ul.nav.navbar-nav.navbar-right
          .li
            =link_to users_path, :class => "navbar-brand" do
              Users
    .container
      =yield
